<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h1 id="appName">{{msg}}</h1>
        <h2 id="tagline">Make and take smart quizzes on-the-go!</h2>
      </div>
    </div>
  </div>

  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-md-6">
        <a id="takeQuizBtn" class="btn btn-primary" href="/quiz-selection" role="button">Take a Quiz</a>
      </div>

      <div class="col-sm-12 col-md-6">
        <a id="makeQuizBtn" class="btn btn-success" href="/create-quiz" role="button">Make a Quiz</a>
      </div>
    </div>
  </div>

</div>




